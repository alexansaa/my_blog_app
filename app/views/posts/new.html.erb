<h1>New post!</h1>
<%= form_with model: [@user, @post], url: user_posts_path(@user), method: :post, local: false, scope: :post do |f| %>
  <div>
    <%= f.label :title, 'Post Title' %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :text, 'Post Text' %>
    <%= f.text_area :text %>
  </div>
  <%= f.submit 'Create Post' %>
<% end %>